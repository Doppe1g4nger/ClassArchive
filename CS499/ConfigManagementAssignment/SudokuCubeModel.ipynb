    "        \n",
    "        new_left_row_val = np.flip(back_row_state[\"val\"])\n",
    "        new_left_row_ori = np.flip(self.rotate_ori_clockwise(self.rotate_ori_clockwise(back_row_state[\"ori\"])))\n",
    "        self._left.set_bottom_row(new_left_row_val, new_left_row_ori)\n",
    "        \n",
